using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace CSharp_Shell
{
   
 public class Program 
    {
 public static void Main()
   {
try
	{
	int a=2;
	int b=0;
	int c=a/b;
	Console.WriteLine(c);
    Console.ReadLine();
	}
catch(Exception e)
	{
	Console.WriteLine("Exception occurred:-"+e);
    Console.ReadLine();
	}
finally
	{
	Console.WriteLine("Exception handled");
    Console.ReadLine();
	}       }    }}
